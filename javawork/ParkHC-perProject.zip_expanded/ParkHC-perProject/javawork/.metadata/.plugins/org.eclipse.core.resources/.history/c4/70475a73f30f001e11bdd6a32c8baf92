package day0621;

public class ExForWhile_10 {

	public static void test1() {
		
		//for문으로 1부터 5까지 합 구하기
		
		int sum = 0;
		for (int i = 1; i<=5; i++) {
			
			sum += i;
			System.out.println("총 합계는 " + sum);
			
		}
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		test1();
		
		
		
		
		
	}

}
