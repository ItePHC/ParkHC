package day0621;

public class ExFor_06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//for문
		// 1~10까지 가로로 출력
		
		for(int i = 1; i<=10; i++) {
			System.out.print(i + " ");
		}
		
		System.out.println();
		
		//10~1까지 가로출력
		
		for(int i = 10; i>=1; i--) {
			System.out.print(i + " ");
		}
		System.out.println();
		
	}

}
