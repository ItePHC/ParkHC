package day0616;

public class PrintfTest_05 {

	public static void main(String[] args) {
		// Printf는 문자열을 구성해서 출력
		//문자열 뒤에 따라오는 데이터(%)를 순서대로 배치한다
		/*
		* %d : 정수
		* %f : 실수
		* %s : 문자열
		*/
		
	int num1 = 30, num2 = 20, num3 = num1 + num2;
	System.out.println(num1 + "+" + num2 + "=" + num3);
	
	System.out.println("num1의 값은 %d, num2의 값은 %d, num3의 값은 %d입니다",num1,num2,num3);
	
	
	}

}
