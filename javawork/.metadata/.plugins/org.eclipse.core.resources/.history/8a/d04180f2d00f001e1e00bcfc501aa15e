package day0621;

public class DoWhile_06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i = 1;
		System.out.println("while문 결과");
		
		while(i<5) {
			//출력 후 증가
			System.out.println(i++);
			
		 // System.out.println(i);
	     // i++;
			
		}
		
		System.out.println();
		
 		// i = 1;
		do {
			System.out.println(i++);
		}while(i>5);
	}

}
