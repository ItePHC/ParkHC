package day0621;

public class QuizFor_09_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Q. 1 2 4 5 7 8 10 _ continue 이용해서 출력
		
		
		int c = 0;
		int csum = 0;
		while(true) {
			++c;
			if(
			c % 2 == 1) {
				continue;
			}
			
			

			csum += c;	
			
			if(c > 100) {
				break;
			}
			
			
		}
			System.out.println("1부터 100까지 중 홀수의 합은 " + csum);
		
	}

}
