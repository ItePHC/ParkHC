package day0616;

public class PrintfTest_06_1 {

	public static void main(String[] args) {
		// printf로 출력하세요
		//안녕하세요 제 이름은 송혜교입니다
		//나이는 25세 입니다
		// %d : 정수
		// %f : 실수
		// %s : 문자열
		
		
		
		String name = args[0];
		String age = args[1]; 
		
		System.out.printf("안녕하세요 제 이름은 %s 입니다. 나이는 %d 입니다." ,name ,age);
		
		
		
	}

}
